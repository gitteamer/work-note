        "[Monokai Night]": {
            //"comments": "#ff6600",
            //"functions": "#FF0000",
            //"keywords": "#FF0000",
            //"numbers": "#FF0000",
            //"strings": "#FF0000",
            //"types": "#FF0000",
            //"variables": "#FF0000",
            "textMateRules": [
                {
                    // 包名
                    "scope": "entity.name.package.go",
                    "settings": {
                        "foreground": "#FD971F"
                    }
                },
                {
                    // 引入包名的别名
                    "scope": "entity.alias.import.go",
                    "settings": {
                        "foreground": "#FD971F"
                    }
                },
                {
                    // 包名使用的地方
                    "scope": "punctuation.other.period.go",
                    "settings": {
                        "foreground": "#FD971F"
                    }
                },
                {
                    // 定义的变量
                    "scope": "variable.other.declaration.go",
                    "settings": {
                        "foreground": "#A6E22E"
                    }
                },
                {
                    // 赋值初始化的变量
                    "scope": "variable.other.assignment.go",
                    "settings": {
                        "foreground": "#A6E22E"
                    }
                },
                {
                    // string类型
                    "scope": "storage.type.string.go",
                    "settings": {
                        "foreground": "#F92672"
                    }
                },
                {
                    // bool类型
                    "scope": "storage.type.boolean.go",
                    "settings": {
                        "foreground": "#F92672"
                    }
                },
                {
                    // 函数
                    "scope": "entity.name.function.go",
                    "settings": {
                        "foreground": "#66D9EF"
                    }
                },
                {
                    // 结构体
                    "scope": "entity.name.type.go",
                    "settings": {
                        "foreground": "#4584A5"
                    }
                }
            ]
        },
    }
}